<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Validation -->
    <Color x:Key="ValidationErrorColor">#1EFF0000</Color>
    
    <!-- Action Colors -->
    <Color x:Key="NeutralActionColor">#00539C</Color>
    <Color x:Key="PositiveActionColor">#388A34</Color>
    <Color x:Key="CreateActionColor">#C27D1A</Color>
    <Color x:Key="NegativeActionColor">#A1260D</Color>
    <Color x:Key="WarningActionColor">#FFCC00</Color>
    <Color x:Key="InformationActionColor">#1BA1E2</Color>

    <!-- Applicaton Colors -->
    <Color x:Key="AdobeApplicationColor">#EE0C0D</Color>
    <Color x:Key="AccessApplicationColor">#89003A</Color>
    <Color x:Key="ExcelApplicationColor">#297733</Color>
    <Color x:Key="OneNoteApplicationColor">#920891</Color>
    <Color x:Key="OutlookApplicationColor">#D77C02</Color>
    <Color x:Key="PowerPointApplicationColor">#CB2404</Color>
    <Color x:Key="WordApplicationColor">#1C428E</Color>
    <Color x:Key="LyncApplicationColor">#0072C6</Color>
    <Color x:Key="OfficeApplicationColor">#EB3B00</Color>

</ResourceDictionary>